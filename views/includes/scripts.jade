script(type='text/javascript'): include ../../public/scripts/jquery.mixitup.js

script.

  // JQuery closure
  (function($){

    // When document has is ready
    $(document).ready(function() {

      // Settings for tooltip
      $(".tooltip_a a").tooltip({
          placement : 'bottom',
          html: true,
          effect: 'slide',
          content: 'loading...',
          open: function (event, ui) {
              $(ui.tooltip).appendTo(this);
          }
      });

      // Initialize mixItUp
      $('#Container').mixItUp();

      // Initialize datepicker
      $('#datepicker').datepicker({
        format: "dd/mm/yyyy"
      });

    });
  })(jQuery);

